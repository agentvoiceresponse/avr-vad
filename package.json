{
  "name": "avr-vad",
  "version": "1.0.4",
  "description": "A Node.js library for Voice Activity Detection using Silero VAD",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node examples/basic-example.ts",
    "test-example": "ts-node examples/test-example.js",
    "inspect": "ts-node scripts/inspect-model.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "keywords": [
    "vad",
    "voice-activity-detection",
    "silero",
    "audio",
    "speech",
    "onnx"
  ],
  "author": {
    "name": "Agent Voice Response",
    "email": "info@agentvoiceresponse.com",
    "url": "https://github.com/agentvoiceresponse"
  },
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^2.7.0",
    "onnxruntime-node": "^1.22.0",
    "wav": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/node-fetch": "^2.6.12",
    "@types/wav": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "files": [
    "lib/**/*",
    "models/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentvoiceresponse/avr-vad.git"
  },
  "bugs": {
    "url": "https://github.com/agentvoiceresponse/avr-vad/issues"
  },
